<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="initialColor" format="color"/>
    <attr name="transparency" format="boolean"/>

    <declare-styleable name="ColorPickerSlidersView">
        <attr name="initialColor"/>
        <attr name="transparency"/>
    </declare-styleable>


    <attr name="suffix" format="string"/>
    <attr name="title" format="string"/>

    <declare-styleable name="SliderWithNumber">
        <attr name="suffix"/>
        <attr name="title"/>
    </declare-styleable>

</resources>
